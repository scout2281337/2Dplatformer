.Inventory {
    width: 24%;
    height: 9%;
    margin-top: 1%;
    margin-right: 1%;
    margin-bottom: 1%;
    margin-left: 1%;
    position: absolute;
    left: 0;
    bottom: 0;
    flex-direction: row;
    justify-content: space-between;
}

.Slot {
    background-color: rgba(255, 255, 255, 0.37);
    flex-grow: 1;
    -unity-text-align: middle-center;
    font-size: 50px;
}
